<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="700.0"
            xmlns="http://javafx.com/javafx/8.0.121" fx:controller="macro303.neptunes.NeptunesController">
	<center>
		<TabPane tabClosingPolicy="UNAVAILABLE">
			<Tab text="Game">
				<GridPane hgap="5" vgap="5.0">
					<Label fx:id="labelName" text="Name" GridPane.columnIndex="0" GridPane.columnSpan="2"
					       GridPane.halignment="CENTER" GridPane.rowIndex="0"/>
					<Label text="Started: " GridPane.columnIndex="0" GridPane.rowIndex="1"/>
					<Label fx:id="labelStarted" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1"/>
					<columnConstraints>
						<ColumnConstraints percentWidth="20"/>
						<ColumnConstraints/>
					</columnConstraints>
					<rowConstraints>
						<RowConstraints/>
						<RowConstraints/>
					</rowConstraints>
					<padding>
						<Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
					</padding>
				</GridPane>
			</Tab>
			<Tab text="Players">
			</Tab>
			<padding>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
			</padding>
		</TabPane>
	</center>
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
	</padding>
</BorderPane>
